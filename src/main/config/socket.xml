<?xml version="1.0" encoding="UTF-8"?>
<socket-app>

	<server-preferences>
		<performance-preferences>
			<connection-time>1</connection-time>
			<latency>1</latency>
			<bandwidth>1</bandwidth>
		</performance-preferences>
		<host-name>127.0.0.1</host-name>
		<port>60000</port>
		<max-connections>10</max-connections>
	</server-preferences>

	<client-preferences>
		<timeout>30</timeout>
		<keep-alive>true</keep-alive>
		<tcp-no-delay>true</tcp-no-delay>
	</client-preferences>

	<servlet>
		<name>TestServler</name>
		<class>net.newcapec.mainUi.web.servlet.WebSourceServlet</class>
	</servlet>

</socket-app>